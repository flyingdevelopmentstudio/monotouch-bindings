    <Member MemberName="Midpoint">
      <MemberSignature Language="C#" Value="public virtual System.Drawing.PointF Midpoint { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype System.Drawing.PointF Midpoint" />
      <MemberType>Property</MemberType>
      <AssemblyInfo>
        <AssemblyVersion>0.0.0.0</AssemblyVersion>
